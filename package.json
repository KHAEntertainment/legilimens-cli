{
  "name": "legilimens-cli",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "typecheck": "pnpm -r run typecheck",
    "lint": "pnpm exec eslint .",
    "test": "pnpm -r run test",
    "test:integration": "vitest run --config vitest.config.ts",
    "start:cli": "pnpm --filter @legilimens/cli start",
    "start:harness": "pnpm --filter @legilimens/harness-service dev"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/decompress": "^4.2.7",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.6.0",
    "vitest": "^1.5.0"
  },
  "pnpm": {
    "packageImportMethod": "auto"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@tavily/core": "^0.5.12",
    "decompress": "^4.2.1"
  }
}
